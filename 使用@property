# 简单限制score输入内容
class Student(object):
    def get_score(self):
        return self._score
    def set_score(self,value):
        if not isinstance(value,int):
            raise ValueError('score must be an integer!')
        if value <0 or value > 100:
            raise ValueError('score must between 0-100')
        self._score = value

s = Student()
s.set_score(60)
print(s.get_score())
s1 = Student()
s1.set_score(1.2)
print(s1.get_score())
s.set_score(3333)
print(s.set_score())
